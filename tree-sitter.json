{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "apache_avro",
      "camelcase": "ApacheAvro",
      "title": "Apache Avro",
      "scope": "source.apache_avro",
      "file-types": [
        "avdl",
        "avsc"
      ],
      "injection-regex": "^apache_avro$",
      "class-name": "TreeSitterApacheAvro"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Unofficial Apache Avro Language Parser, it includes IDL and AVSC",
    "authors": [
      {
        "name": "Victor Quiroz",
        "email": "git@victorhqc.com",
        "url": "https://victorhqc.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/victorhqc/tree-sitter-apache-avro"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}